<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.example.suno-watch</string>

  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/python3</string>
    <string>REPLACE_WITH_ABSOLUTE_PATH_TO_REPO/scripts/watch_m3u.py</string>
